extends ../layout

block content
   div.container
      div.row
         div.col-xs-12.col-sm-8.col-sm-offset-2.col-md-4.col-md-offset-4
            h1.text-center.title #{title}
            - each user, i in users
               div.content-wall
                  p Name: #{user.username}
                  p Email: #{user.email}
                  p First Name: #{user.firstName}
                  p Last Name: #{user.lastName}